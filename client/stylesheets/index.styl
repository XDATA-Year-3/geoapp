@import 'nib'

html, body
  height 100%
  overflow hidden

*
  font-family 'Droid Sans',sans-serif
  font-smoothing antialiased

body
  padding 0
  margin 0
  word-wrap break-word

a
  cursor pointer
  text-decoration none !important

#ga-main-map
  width 100%
  height 100%
  position absolute

#ga-main-page
  position absolute
  margin 3px
